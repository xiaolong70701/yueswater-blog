# 載入必要的套件
library(ggplot2)
# 定義效用函數
utility <- function(x, y, alpha) {
return(x^alpha * y^(1-alpha))
}
# 創建網格點
x <- seq(1, 10, length.out = 100)
y <- seq(1, 10, length.out = 100)
df <- expand.grid(x = x, y = y)
# 設定alpha的值
alpha <- 0.5
# 計算對應效用值
df$Z <- with(df, utility(x, y, alpha))
# 繪製無異曲線
ggplot(df, aes(x, y, z = Z)) +
geom_contour(breaks = seq(min(df$Z), max(df$Z), length.out = 10), col = "black") +
labs(x = "x", y = "y", title = "Indifference Curve")
